输出0到100内的奇数
#include <stdio.h>

int main()
{  
	int i=1;
	while(i<=100)
	{
		printf("%d",i);
		i+=2;
	}






	/*int i=1;
	while(i<=100)
	{
        if(i%2 != 0)
			printf("%2d \n ",i);
		i++;
	}*/
    return 0;

}





输出星期
#include <stdio.h>

int main()
{  
	int i=0;
	scanf("%d",&i);
	switch(i)
	{
	case 1:
		printf("星期1\n");break;
	case 2:
		printf("星期2\n");break;
    case 3:
		printf("星期3\n");break;
    case 4:
		printf("星期4\n");break;
    case 5:
		printf("星期5\n");break;
    case 6:
		printf("星期6\n");break;
    case 7:
		printf("星期7\n");break;
 	}



输出1到3阶乘的和
#include <stdio.h>

int main()
{  
	int i=0;
	int n=0;
	int ret=1;
	int sum=0;
//	scanf("%d",&n);
	for(n=1;n<=3;n++)
	{ ret=1;
	  for(i=1;i<=n;i++)
	   {
		  ret=ret * i;
	   }
	 sum=sum+ret; //printf("%d",ret);
	}
	
	printf("%d",sum);
    return 0;
}

