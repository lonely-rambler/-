最小公约数
#include <stdio.h>


int main()
{
	int a=0;
	int b=0;
	int m=0;
	scanf("%d%d",&a,&b);
	while(m=a%b)
	{
		a=b;
		b=m;
	}
	printf("%d",b);
}


